<!DOCTYPE html>
<html>
<head>
    <title>Simple Map</title>
    <script type="text/javascript" src="https://www.amcharts.com/lib/3/amcharts.js"></script>
    <script type="text/javascript" src="https://www.amcharts.com/lib/3/serial.js"></script>
    <script type="text/javascript" src="https://www.amcharts.com/lib/3/themes/none.js"></script>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
    <style>
        #map {
            height: 100%;
        }
        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }

        .toolbar {
            display: block;
            position: fixed;
            bottom: 0;
            height: 0;
            width: 100%;
            background: white;
            z-index: 30;
            overflow-y: scroll;
            transition: height 1s;
        }
        .chartdiv {
            width: 100%;
            height: 300px;
        }
        
    </style>
</head>
<body>
    @RenderBody()
<script src="/js/program.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC0R-OCTZIIM9f9yfrHK4Uguj9gsQFBFBU&libraries=visualization&callback=initMap"
            async defer></script>
</body>
</html>
